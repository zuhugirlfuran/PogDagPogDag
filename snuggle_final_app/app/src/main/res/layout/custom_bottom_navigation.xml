<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    android:clipToPadding="false">

    <!-- Bottom Frame -->
    <FrameLayout
        android:id="@+id/bottom_frame2"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:clipChildren="false"
        android:clipToPadding="false"
        app:layout_constraintBottom_toBottomOf="@id/bottom_frame2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <View
            android:id="@+id/backIndicator2"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/bottom_circle_background2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- Navigation Bar -->
<!--    <androidx.constraintlayout.widget.ConstraintLayout-->
<!--        android:id="@+id/bottom_navigation2"-->
<!--        android:layout_width="0dp"-->

<!--        android:layout_height="120dp"-->
<!--        android:background="@android:color/transparent"-->
<!--        android:clipChildren="false"-->
<!--        android:clipToPadding="false"-->
<!--        app:layout_constraintBottom_toBottomOf="@id/bottom_frame2"-->
<!--        app:layout_constraintEnd_toEndOf="parent"-->
<!--        app:layout_constraintStart_toStartOf="parent">-->

        <!-- 제품 Tab -->
        <LinearLayout
            android:id="@+id/product"
            android:layout_width="0dp"

            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:gravity="center"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/scanner"
            app:layout_constraintStart_toStartOf="parent">

            <FrameLayout
                android:layout_width="50dp"
                android:layout_height="50dp"

                android:layout_gravity="center"
                android:clipChildren="false"
                android:clipToPadding="false">

                <View
                    android:id="@+id/productIndicator2"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:background="@drawable/bottom_navigation_active_btn"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/productIcon2"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_gravity="center"
                    android:src="@drawable/product_icon" />
            </FrameLayout>

            <TextView
                android:id="@+id/productLabel2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="제품"
                android:textSize="12sp" />
        </LinearLayout>

        <!-- 스캐너 Tab -->
        <LinearLayout
            android:id="@+id/scanner"
            android:layout_width="0dp"

            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:gravity="center"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/home"
            app:layout_constraintStart_toEndOf="@id/product">

            <FrameLayout
                android:layout_width="50dp"
                android:layout_height="50dp"

                android:layout_gravity="center"
                android:clipChildren="false"
                android:clipToPadding="false">

                <View
                    android:id="@+id/scannerIndicator2"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:background="@drawable/bottom_navigation_active_btn"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/scannerIcon2"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_gravity="center"
                    android:src="@drawable/scanner_icon" />
            </FrameLayout>

            <TextView
                android:id="@+id/scannerLabel2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="스캐너"
                android:textSize="12sp" />
        </LinearLayout>

        <!-- 홈 Tab -->
        <LinearLayout
            android:id="@+id/home"
            android:layout_width="0dp"

            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:gravity="center"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/chatbot"
            app:layout_constraintStart_toEndOf="@id/scanner">

            <FrameLayout
                android:layout_width="50dp"
                android:layout_height="50dp"

                android:layout_gravity="center"
                android:clipChildren="false"
                android:clipToPadding="false">

                <View
                    android:id="@+id/homeIndicator2"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:background="@drawable/bottom_navigation_active_btn"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/homeIcon2"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_gravity="center"
                    android:src="@drawable/home_icon" />
            </FrameLayout>

            <TextView
                android:id="@+id/homeLabel2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="홈"
                android:textSize="12sp" />
        </LinearLayout>

        <!-- 챗봇 Tab -->
        <LinearLayout
            android:id="@+id/chatbot"
            android:layout_width="0dp"

            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:gravity="center"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/mypage"
            app:layout_constraintStart_toEndOf="@id/home">

            <FrameLayout
                android:layout_width="50dp"
                android:layout_height="50dp"

                android:layout_gravity="center"
                android:clipChildren="false"
                android:clipToPadding="false">

                <View
                    android:id="@+id/chatbotIndicator2"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:background="@drawable/bottom_navigation_active_btn"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/chatbotIcon2"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_gravity="center"
                    android:src="@drawable/chatbot_icon" />
            </FrameLayout>

            <TextView
                android:id="@+id/chatbotLabel2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="챗봇"
                android:textSize="12sp" />
        </LinearLayout>

        <!-- 마이페이지 Tab -->
        <LinearLayout
            android:id="@+id/mypage"
            android:layout_width="0dp"

            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:gravity="center"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/chatbot">

            <FrameLayout
                android:layout_width="50dp"
                android:layout_height="50dp"

                android:layout_gravity="center"
                android:clipChildren="false"
                android:clipToPadding="false">

                <View
                    android:id="@+id/mypageIndicator2"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:background="@drawable/bottom_navigation_active_btn"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/mypageIcon2"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_gravity="center"
                    android:src="@drawable/mypage_icon" />
            </FrameLayout>

            <TextView
                android:id="@+id/mypageLabel2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="마이페이지"
                android:textSize="12sp" />
        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
<!--</androidx.constraintlayout.widget.ConstraintLayout>-->
