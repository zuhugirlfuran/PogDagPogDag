<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
	
	
<mapper namespace="com.ssafy.snuggle.model.dao.TaggingDao">

	<select id="selectByTaggingId" parameterType="string" resultType="Tagging">
		SELECT *
		FROM t_tagging
		WHERE tagging_id = #{taggingId}
	</select>
	
</mapper>